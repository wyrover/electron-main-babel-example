{
  "name": "electron-main-babel-example",
  "productName": "electron-main-babel-example",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=main nodemon --watch ./src --exec electron --debug --app -r @babel/register ./src/index.js",
    "service": "cross-env ELECTRON_RUN_AS_NODE=1 NODE_ENV=main nodemon --watch ./src --exec electron -r @babel/register ./src/service.js",
    "install": "electron-rebuild",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src"
  },
  "nodemonConfig": {
    "ignore": [
      "src/*.json"
    ],
    "delay": "2500"
  },
  "keywords": [],
  "author": {
    "name": "wyrover",
    "email": "wyrover@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_main_babel_example"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "external-ip": "^2.3.1",
    "get-port": "^5.1.1",
    "humanize-duration": "^3.23.1",
    "iconv-lite": "^0.6.2",
    "jayson": "^3.3.3",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
    "lodash": "^4.17.19",
    "node-machine-id": "^1.1.12",
    "node-schedule": "^1.3.2",
    "prettysize": "^2.0.0",
    "shelljs": "^0.8.4",
    "systeminformation": "^4.27.11",
    "urlencode": "^1.1.0",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "6.0.0-beta.52",
    "@electron-forge/maker-zip": "6.0.0-beta.52",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "electron": "9.1.1",
    "electron-rebuild": "^1.11.0",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.4"
  }
}
